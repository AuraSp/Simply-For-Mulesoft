#%RAML 1.0 DataType

type: object
properties:
  id:
    type: integer
    minimum: 1
    required: false
  name:
    type: string
  type:
    type: string
    enum:
      - business
      - personal
  address:
    type: string
  airMiles:
    type: integer
    minimum: 0
  createdAt:
    type: datetime
    required: false